{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "{{if .Root}}âš¡ {{end}} {{ .Shell }} ",
    "foreground": "#FFD700",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[<#f0ff0f>{{ .UserName }}</><#ffffff>@</><#55ff55>{{ .HostName }}</>] ",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "style": "unique"
          },
          "template": "<#ffffff>[</>{{ .Path }}<#ffffff>]</> ",
          "foreground": "#9296e3",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "<#ffffff>[</>{{ .HEAD }}{{ if .Staging.Changed }}<#00AA00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}<#00AA00> |</>{{ end }}{{ if .Working.Changed }}<#D75F00> \uf044 {{ .Working.String }}</>{{ end }}{{ if gt .StashCount 0 }}<#D75F00> \uf692 {{ .StashCount }}</>{{ end }}<#ffffff>] </>",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}<#3C873A>\uf487</><#ffffff> {{.Version}}{{ end }}|{{ if .Name }}{{ .Name }}{{ end }}{{ end }})</>",
          "foreground": "#ebf1f6",
          "type": "project",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>n</> ",
            "yarn_icon": " <#348cba>y</>"
          },
          "template": "<#ffffff>(</>{{if .PackageManagerIcon}}{{.PackageManagerIcon}} {{ end }}{{.Full}}<#ffffff>)</>",
          "foreground": "#3C873A",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#FFE873",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>\ue769 {{ .Full }}<#ffffff>)</>",
          "foreground": "#ffffff",
          "type": "perl",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#ec2729",
          "type": "java",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#0d6da8",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#06aad5",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#925837",
          "type": "rust",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#ce092f",
          "type": "angular",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#9c1006",
          "type": "ruby",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "foreground": "#5398c2",
          "type": "azfunc",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570$",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>!(</>#<#ffffff>)</>",
          "foreground": "#FF9248",
          "type": "root",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
